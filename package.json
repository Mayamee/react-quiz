{
  "name": "react-quiz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.99",
    "@mui/material": "^5.10.2",
    "@mui/styled-engine": "^5.10.2",
    "@reduxjs/toolkit": "^1.8.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.27.2",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.53.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "prettify": "prettier --write \"(src|config|public)/**/*.{js,jsx,ts,tsx,json,css,scss,sass,md,html}\"",
    "dev": "cross-env PORT=8081 react-scripts start",
    "build": "yarn build:front && yarn build:server",
    "dev:server": "yarn build:front && yarn watch:server",
    "watch:server": "cross-env PORT=8081 webpack --config config/webpack.server.config.js --watch",
    "build:front": "cross-env BUILD_PATH='./build/client' react-scripts build",
    "build:server": "cross-env MODE=production webpack --config config/webpack.server.config.js",
    "test:front": "react-scripts test",
    "update": "yarn install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "nodemon-webpack-plugin": "^4.8.1",
    "prettier": "^2.7.1",
    "sass-loader": "^13.1.0",
    "url-loader": "^4.1.1",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
